@model Veterinary_App_Semos.Models.Vaccine

@{
    ViewData["Title"] = "Vaccine Details";
}

<link rel="stylesheet" href="~/css/details.css" />

<div class="container mt-4">
    <div class="card owner-card">
        <div class="text-center owner-title">
            💉 Vaccine Information 💉
        </div>
   
        <hr>
        <div class="card-body p-3">
            <p><strong>@Html.DisplayNameFor(model => model.Name):</strong> @Html.DisplayFor(model => model.Name)</p>
        </div>
        <div class="card-footer text-center p-2 footer-bg">
            <a class="btn btn-sm edit-btn" asp-action="Edit" asp-route-id="@Model?.VaccineId">Edit</a>
            <a class="btn btn-sm back-btn" asp-action="Index">Back</a>
        </div>
    </div>
</div>